<h1>Test</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Count of questions</th>
  </tr>

  <tr>
    <td><%= @test.title %></td>
    <td><%= @test.level %></td>
    <td><%= @test.questions.count %></td>
  </tr>
</table>

<h2>Questions</h2>

<table>
  <tr>
    <th>Text</th>
    <th colspan="3">Actions</th>
  </tr>

  <%= render @test.questions %>
</table> 
<%= link_to 'Create', new_test_question_path(@test) %>
<%= link_to 'Back', tests_path %>
